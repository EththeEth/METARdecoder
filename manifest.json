{
    "name": "METAR decoder",
    "description": "Having trouble understanding METAR reports? With METAR decoder, you can figure out what your report says instantly!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"

      },
      "permissions": ["storage", "activeTab", "scripting"],
      "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/windsock16.png",
            "32": "/images/windsock32.png",
            "48": "/images/windsock48.png",
            "128": "/images/windsock128.png"
          }
      },
      "icons": {
        "16": "/images/windsock16.png",
        "32": "/images/windsock32.png",
        "48": "/images/windsock48.png",
        "128": "/images/windsock128.png"
      },
      "content_scripts": [{
        "matches": [ "http://*/*", "https://*/*"],
        "js": ["jquery-3.6.0.min.js"]
    }]
  }